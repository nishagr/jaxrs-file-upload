package com.gs.utils;

import com.gs.models.RequestInfo;

public class FileAdapter {
	public FileHandler getFileHandler(String extension, RequestInfo request) {
		if (extension == null)
			return null;
		switch (extension.toLowerCase()) {
		case "csv":
			return new CSVFileHandler(request);
		case "xls":
		case "xlsx":
			return new ExcelFileHandler(request);
		default:
			return null;
		}
	}
}
